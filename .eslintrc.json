{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"mocha": true
	},
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"plugins": [
		"prettier"
	],
	"rules": {
		"import/no-named-as-default": [
			0
		],
		"jsx-a11y/href-no-hash": [
			0
		],
		"no-shadow": "off",
		"react/jsx-indent": [
			2,
			"tab"
		],
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"react/jsx-one-expression-per-line": [
			0
		],
		"react/prop-types": [
			0
		],
		"prettier/prettier": "error"
	}
}